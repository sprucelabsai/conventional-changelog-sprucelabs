{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "test",
			"program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
			"args": [
				"--timeout",
				"30000",
				"test/test.js",
			],
			"cwd": "${workspaceFolder}",
			"skipFiles": [
				"<node_internals>/**"
			],
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
		}
	]
}
